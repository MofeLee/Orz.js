{
    "rules": {
        "indent": [
            2,
            4
        ],
        "quotes": [
            2,
            "single"
        ],
        "linebreak-style": [
            2,
            "unix"
        ],
        "semi": [
            2,
            "always"
        ],
        "no-console": 1
    },
    "env": {
        "es6": true,
        "node": true,
        "browser": true
    },
    "extends": "eslint:recommended",
    "ecmaFeatures": {
    },
    "globals": {
        "Orz": false
        "$": false,
        "app": false,
        "describe": false,
        "it": false,
        "expect": false,
        "context": false,
        "beforeEach": false,
        "afterEach": false
    }
}
